#!/usr/bin/env nextflow

nextflow.enable.dsl = 2

////////////////////////////////////////////////////
/* --              Parameter setup             -- */
////////////////////////////////////////////////////

////////////////////////////////////////////////////
/* --              IMPORT MODULES              -- */
////////////////////////////////////////////////////

include { trycycler} from '../tools/trycycler/trycycler'

////////////////////////////////////////////////////
/* --           RUN MAIN WORKFLOW              -- */
////////////////////////////////////////////////////

workflow Trycycler_Full {
    take:
        trycycler_input

    main:
        trycycler(
            trycycler_input
        )
            
    emit:
        trycycler.out.consensus

}